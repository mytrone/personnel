<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gson.linyun.model.mapper.clockingin.IClockinginDetailsMapper">

    <!--<select id="selectAll" resultType="cn.gson.linyun.model.pojos.clockingin.ClockinginDetails">

    </select>-->
    <resultMap id="DetaMap" type="cn.gson.linyun.model.pojos.clockingin.ClockinginDetails">
        <id column="details_id" property="detailsId" />
        <result column="details_money" property="detailsMoney" />
        <result column="details_name" property="detailsName" />
        <result column="details_receipt" property="detailsReceipt" />
    </resultMap>
    <select id="selectById" resultMap="DetaMap" parameterType="java.lang.Integer">
        select details_id,details_money,details_name,details_receipt from clockingin_details where reimburse_id=#{resId};
    </select>

    <!--新增报销详情 insertDetails-->
    <insert id="insertDetails" parameterType="cn.gson.linyun.model.pojos.clockingin.ClockinginDetails">
        insert into clockingin_details values (
        null,#{reimburseId.reimburseId},#{detailsName},
        #{detailsMoney},#{detailsReceipt}
        )
    </insert>

</mapper>